{
  "connection": "default",
  "collectionName": "languages",
  "info": {
    "name": "language",
    "description": "A list of programming languages "
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "name": {
      "type": "string",
      "maxLength": 50,
      "unique": true,
      "required": true
    },
    "description": {
      "type": "text"
    },
    "Active": {
      "default": true,
      "type": "boolean"
    }
  }
}